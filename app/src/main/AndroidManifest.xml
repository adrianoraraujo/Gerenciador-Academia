<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.br.gerenciadordetreino">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.Manifest.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:windowSoftInputMode="stateHidden"
            android:configChanges="keyboardHidden"
            android:name=".view.HomeActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:windowSoftInputMode="stateHidden"
            android:configChanges="keyboardHidden"
            android:name=".view.CadastroActivity_"
            android:theme="@style/AppThemeFull" />
        <activity
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboardHidden"
            android:theme="@style/PopupTheme"
            android:name=".view.CadastroEquipamentoActivity_"></activity>

        <activity
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboardHidden"
            android:theme="@style/PopupTheme"
            android:name=".view.CadastroTreinoActivity_"></activity>
    </application>

</manifest>